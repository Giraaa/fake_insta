<h1>길스타그램</h1>
<table class="table table-hover text-center">
  <thead>
    <tr>
      <th width="15%">글번호</th>
      <th>제목</th>
    </tr>
  </thead>
  <tbody>
    <% @post.each do |post| %>
    <tr class="post" data-value="<%= post.id %>">
      <td><%= post.id %></td>
      <td class="title" data-id="<%= post.id %>"><%= post.title %></td>
      <td>
        <a href="/posts/<%= post.id %>">내용</a>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
<form>
  <a href="/posts/new" class="btn btn-warning">글쓰기</a>




</form>
